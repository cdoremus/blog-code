{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--inspect-brk",
        "-A",
        "queue.ts"
      ],
      "attachSimplePort": 9229,
      "stopOnEntry": true
    }
  ]
}
